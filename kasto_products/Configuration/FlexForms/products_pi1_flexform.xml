<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Main</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <TCEforms>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.mode</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <default>Product->list;Product->detail;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter</default>
                                <items type="array">
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.mode.list</numIndex>
                                        <numIndex index="1">Product->list;Product->detail;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.mode.detail</numIndex>
                                        <numIndex index="1">Product->detail;Product->list;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.mode.tabs</numIndex>
                                        <numIndex index="1">Product->tabs;Product->list;Product->detail;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </switchableControllerActions>
                    <settings.type>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->list;Product->detail;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.type_to_show</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <foreign_table>tx_kastoproducts_domain_model_type</foreign_table>
                                <foreign_table_where>AND tx_kastoproducts_domain_model_type.hidden=0 AND tx_kastoproducts_domain_model_type.deleted=0</foreign_table_where>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                            </config>
                        </TCEforms>
                    </settings.type>
                    <settings.isUsed>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->list;Product->detail;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.show_is_used</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.isUsed>
                    <settings.itemsPerPage>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->list;Product->detail;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.item_per_page</label>
                            <config>
                                <type>input</type>
                                <eval>num,int</eval>
                                <default>12</default>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.itemsPerPage>
                    <settings.servicePID>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->detail;Product->list;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.service_detail_pid</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.servicePID>
                    <settings.consultantsPID>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->detail;Product->list;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.consultants_pid</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.consultantsPID>
                    <settings.contactPID>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->detail;Product->list;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.contact_pid</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.contactPID>
                    <settings.detailPID>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->list;Product->detail;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.detail_page</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <eval>required</eval>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.detailPID>
                    <settings.backPID>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->detail;Product->list;Product->tabs;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.back_page</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.backPID>
                    <settings.tabsSawMachines>
                        <TCEforms>
                            <displayCond>FIELD:switchableControllerActions:=:Product->tabs;Product->list;Product->detail;Product->ajaxLoadMore;Product->ajaxSawMachinesFilter;Product->pdf</displayCond>
                            <label>LLL:EXT:kasto_products/Resources/Private/Language/locallang_be.xlf:flexforms_general.product.products_to_show_in_tabs</label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <foreign_table>tx_kastoproducts_domain_model_product</foreign_table>
                                <foreign_table_where> AND tx_kastoproducts_domain_model_product.sys_language_uid=0 AND tx_kastoproducts_domain_model_product.type=1</foreign_table_where>
                                <size>10</size>
                                <maxitems>6</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                                <enableMultiSelectFilterTextfield>1</enableMultiSelectFilterTextfield>
                            </config>
                        </TCEforms>
                    </settings.tabsSawMachines>
                </el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>